<!-- % User defined (D)IR MRI T1/T2 settings for IR_Calculator. WM/GM/CSF, MS(WML) and Fat.         -->
<!--    - These values are read at startup by the IR_SetRelaxTimes IR-Calc module.                  -->
<!--    - The user values can be selected in the IR-Calculator as "T1s" option 3 or 4.              -->
<!--    - If you change them while IR-Calc is running, press 'r' to refresh the values.             -->
<UserRelaxTimes>
     <UserTimes_1>                      <!-- "1.5"T*(Custom): You can add your own times here.      -->
        <B0sys>      1.5    </B0sys>    <!--  #.# T (Used for display only)                         -->
        <T1_WM>    660.0    </T1_WM>    <!--  1.5 T GE source value = 660 ms                        -->
        <T1_GM>   1200.0    </T1_GM>    <!--  1.5 T --"--           = 1200 ms                       -->
        <T1_CSF>  4308.0    </T1_CSF>   <!--  1.5 T --"--           = 4270 ms; Sie uses ~4308 ms?   -->
        <T1_MS>   1200.0    </T1_MS>    <!--  Estimate T1_WML ~ T1_GM?                              -->
        <T1_Fat>   192.0    </T1_Fat>   <!--  1.5 T GE source value = 192 ms                        -->

        <T2_WM>     80.0    </B0sys>    <!--  1.5 T --"--           =   80                          -->
        <T2_GM>     95.0    </T2_GM>    <!--  1.5 T --"--           =   95 ms%                      -->
        <T2_CSF>  3500.0    </T2_CSF>   <!--  1.5 T --"--           = 3500 ms                       -->
        <T2_MS>    100.0    </T2_MS>    <!--  1.5 T Alsop           =   60 ms (meas. 100 ms @3T)    -->
        <T2_Fat>    60.0    </T2_Fat>   <!--  ??? This is pure guesswork for now...                 -->
    </UserTimes_1>
     <UserTimes_2>                      <!-- "3.0"T*(Custom): You can add your own times here.      -->
        <B0sys>      3.0    </B0sys>    <!--  #.# T (Used for display only)                         -->
        <T1_WM>    850.0    </T1_WM>    <!--  3.0 T GE source value =  825 ms                       -->
        <T1_GM>   1400.0    </T1_GM>    <!--  3.0 T --"--           = 1400 ms                       -->
        <T1_CSF>  5000.0    </T1_CSF>   <!--  3.0 T --"--           = 4270 ms (5000 for DIR?!)      -->
        <T1_MS>   1400.0    </T1_MS>    <!--  3.0 T Alsop           = 1350 ms                       -->
        <T1_Fat>   250.0    </T1_Fat>   <!--  3.0 T GE source value =  250 ms                       -->

        <T2_WM>     60.0    </B0sys>    <!--  3.0 T --"--           =   60 ms                       -->
        <T2_GM>     70.0    </T2_GM>    <!--  3.0 T --"--           =   70 ms                       -->
        <T2_CSF>  2400.0    </T2_CSF>   <!--  3.0 T --"--           = 2400 ms                       -->
        <T2_MS>    100.0    </T2_MS>    <!--  1.5 T Alsop           =   60 ms (meas. 100 ms @3T)    -->
        <T2_Fat>    60.0    </T2_Fat>   <!--  ??? This is pure guesswork for now...                 -->
    </UserTimes_2>
</UserRelaxTimes>
